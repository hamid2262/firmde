<table  class="table table-striped">
  <thead>
    <tr>
      <th>name</th>
      <th>company</th>
      <th>email, tel</th>
      <th>address</th>
      <th>message</th>
      <th>created_at</th>
      <th>IP</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.name %></td>
        <td><%= contact.company %></td>
        <td><%= contact.email %> <br><%= "tel: #{contact.tel}" if contact.tel.present? %></td>
        <td><%= contact.address %></td>
        <td><%= contact.message %></td>
        <td><%= contact.created_at.strftime('%d.%m.%Y-%H:%M') %></td>
        <td><%= contact.ip %></td>
        <% if current_user.is_superadmin? %>          
          <td><%= link_to 'Destroy', contact_path(contact) , method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>